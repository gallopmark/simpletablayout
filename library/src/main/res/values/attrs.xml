<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TabLayout">
        <attr name="tab_selectedTextColor" format="color" />
        <attr name="tab_unSelectTextColor" format="color" />
        <attr name="tab_width" format="dimension" />
        <attr name="tab_space_equal" format="boolean" />
        <attr name="tab_drawableWidth" format="dimension" />
        <attr name="tab_drawableHeight" format="dimension" />
        <attr name="tab_drawableGravity" format="enum">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="tab_drawablePadding" format="dimension" />
        <attr name="tab_textSize" format="dimension" />
        <attr name="tab_textStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
            <enum name="bold" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="tab_selectedTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
            <enum name="bold" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="tab_unSelectTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
            <enum name="bold" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="tab_selectedTextSize" format="dimension" />
        <attr name="tab_unSelectTextSize" format="dimension" />
        <attr name="tab_animEnabled" format="dimension" />
        <attr name="tab_reselectedAnimEnabled" format="boolean" />
        <attr name="tab_animIsRecycle" format="boolean" />
        <attr name="tab_animDuration" format="integer" />
        <attr name="tab_alphaStart" format="float" />
    </declare-styleable>
</resources>